@model List<PointOfSales.Models.Category>

    <h2>Category</h2>
<p>
    <a href="/Category/Create">Add Category</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>No</th>
            <th>Nama</th>
            <th>CreateDate</th>
            <th>UpdateDate</th>
            <th>Action</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.CreatedDate</td>
                <td>@item.UpdatedDate</td>
                <td>
                    <a href="/Category/Edit/@item.Id">Edit</a>
                    <a href="/Category/Delete/@item.Id">Delete</a>
                </td>

            </tr>

        }
    </tbody>
</table>